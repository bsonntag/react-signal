{
  "name": "react-signal",
  "version": "0.1.1",
  "description": "Send messages between components.",
  "keywords": [
    "react",
    "signal",
    "message"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "repository": "git@github.com:bsonntag/react-signal.git",
  "author": "Benjamim Sonntag <benjamimsonntag@gmail.com>",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --project tsconfig.json",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run type-check && npm run build:js",
    "build:js": "rm -rf dist && babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.test.tsx\"",
    "format": "npx prettier --write \"./**/*.{ts,tsx,json}\"",
    "lint": "eslint . && npx prettier --check \"./**/*.{ts,tsx,json}\"",
    "test": "jest",
    "test:watch": "jest --watch --notify",
    "version": "npm run build && git add dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@bsonntag/eslint-config": "^0.6.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "lint-staged": "^10.1.7",
    "prettier": "^2.5.1",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  }
}
